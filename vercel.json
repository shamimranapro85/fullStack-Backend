{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "shaim",
          "value": "yourCookie=cookieValue; Path=/; HttpOnly; Secure; SameSite=None"
        }
      ]
    }
  ]
}
